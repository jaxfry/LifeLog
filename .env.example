# Copy this file to .env and fill in your actual values

# Gemini API Key for AI processing
GEMINI_API_KEY=your_gemini_api_key_here

# Frontend development API endpoint
VITE_API_BASE=http://localhost:8000

# Postgres database connection string
DATABASE_URL=postgresql+asyncpg://lifelog:lifelogpassword@localhost:5432/lifelog

# RabbitMQ connection (primarily for local dev when not using Docker Compose)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=user
RABBITMQ_PASS=password
RABBITMQ_QUEUE=lifelog_events_queue

# For ingestion_service (if run outside Docker Compose, or for clarity)
# These are typically overridden by Docker Compose environment settings for containerized deployment
INGESTION_RABBITMQ_HOST=rabbitmq # or localhost if run outside compose
INGESTION_RABBITMQ_PORT=5672

# For processing_service (if run outside Docker Compose, or for clarity)
# These are typically overridden by Docker Compose environment settings for containerized deployment
PROCESSING_RABBITMQ_HOST=rabbitmq # or localhost if run outside compose
PROCESSING_RABBITMQ_PORT=5672
PROCESSING_POSTGRES_HOST=postgres # or localhost if run outside compose
PROCESSING_POSTGRES_USER=lifelog
PROCESSING_POSTGRES_PASSWORD=lifelogpassword
PROCESSING_POSTGRES_DB=lifelog
PROCESSING_POSTGRES_PORT=5432
